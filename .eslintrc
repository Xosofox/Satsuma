// Use this file as a starting point for your project's .eslintrc.
// Copy this file, and add rule overrides as needed.
{
  "extends": "google",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "rules": {
    "indent": [
      "error",
      4,
      {
        "SwitchCase": 1
      }
    ],
    "padded-blocks": [
      2,
      {
        "classes": "always",
        "blocks": "never"
      }
    ],
    //    "semi": [
    //      "error",
    //      "always"
    //    ],
    "max-len": [
      "error",
      120
    ],
    "require-jsdoc": "off",
    "valid-jsdoc": "off",
    "guard-for-in": "off",
    "@babel/semi": "error"
  },
  "plugins": [
    "@babel"
  ]
}
